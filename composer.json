{
	"name": "gsbabaev/test-red-soft",
	"type": "library",
	"description": "class library for json request product as category tree.",
	"keywords": ["json","product", "categories"],
	"license": "MIT",
	"authors": [
		{
			"name": "Alexander Babaev",
			"email": "gs.babaev@yandex.ru",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"kvf77/dbtree": "@stable",
		"colshrapnel/safemysql": "@stable"
	},
	"autoload": {
		"psr-4": {
			"TestRedSotf\\" : "Src"
		}
	},
	"repositories": [        
        {
            "type": "vcs",
            "url": "https://github.com/gsbabaev/test-red-soft.git"
        },
	{
            "type": "vcs",
            "url": "https://github.com/kvf77/DbTree.git"
        },
	"DbTree": {
            "type": "package",
            "package": {
                "name": "kvf77/DbTree",
                "version": "@stable",
                "source": {
                    "url": "https://github.com/kvf77/DbTree",
                    "type": "git"
                },
                "autoload": {
                    "classmap": ["lib"]
                }
            }
        }]
}
